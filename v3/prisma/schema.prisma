generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "sqlite"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

model WaveClaim {
  id     Int    @id @default(autoincrement())
  name   String
  minute Int
  day    Int
  x      Float
  y      Float

  @@index([minute, day])
}
